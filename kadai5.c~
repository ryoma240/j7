#include <stdio.h>
#include <stdlib.h>
#include <math.h>

#defin N 3;

typedef double matrix[N][N];
typedef double vector[N];

void make_matrix_M(matrix m){
  m[0][0] = 5;  m[0][1] = -7; m[0][2] = -7;
  m[1][0] = -4; m[1][1] = 8;  m[1][2] = 7;
  m[2][0] = 4;  m[2][1] = -10;m[2][2] = -9;

}

void make_vector_V(vector v){
  v[0] = 1;
  v[1] = 2;
  v[2] = 3;
}

